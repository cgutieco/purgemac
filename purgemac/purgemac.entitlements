<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!--
    IMPORTANT: App Sandbox is DISABLED.
    This app needs unrestricted access to ~/Library to scan for residual files.
    This is intentional and required for the app's core functionality.
    -->
    <key>com.apple.security.app-sandbox</key>
    <false/>
    
    <!-- Allow read/write to user-selected files (for when user drags apps) -->
    <key>com.apple.security.files.user-selected.read-write</key>
    <true/>
</dict>
</plist>
